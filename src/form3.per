LAYOUT (TEXT="To the Right",MINWIDTH=60)
GRID
{
[title                           |logo         ]
[                                |sp|date      ]
}
END -- GRID

ATTRIBUTES
LABEL TITLE = FORMONLY.title, SIZEPOLICY=FIXED,STYLE="title";

IMAGE sp : spacer1, IMAGE="spacer", STRETCH=X, STYLE="noborder";

IMAGE logo : logo, IMAGE="logo", STYLE="noborder right",STRETCH=X;
LABEL date = FORMONLY.date,STYLE="noborder right", JUSTIFY=RIGHT;
